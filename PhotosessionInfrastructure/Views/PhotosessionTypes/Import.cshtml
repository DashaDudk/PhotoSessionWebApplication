@{
    ViewData["Title"] = "Імпорт даних для типів фотосесій";
}

<h1>Імпорт</h1>

<div>
    <h3>Оберіть excel-файл для завантаження</h3>
    @using (Html.BeginForm("Import", "PhotosessionTypes", FormMethod.Post, new { enctype = "multipart/form-data", id = "frm-excel" }))
    {
        <div>
            Завантажте Excel-файл:
            <input type="file" accept=".xlsx" id="fileInput" maxlength="5000000" name="fileExcel">
            <div>
                <input type="submit" value="Завантажити" />
            </div>
        </div>
    }
</div>